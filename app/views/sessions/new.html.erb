<h2>Log In</h2>
<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
  <%= label_tag(:name, "Enter user's name:") %>
  <%= text_field_tag(:name) %><br><br>
  <%= label_tag(:password, "Enter user's password") %>
  <%= password_field_tag(:password) %><br><br>
  <%= submit_tag("Log In") %>
<% end %>